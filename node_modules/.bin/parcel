#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules/parcel/lib/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules/parcel/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules/parcel/lib/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules/parcel/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/parcel@2.12.0_@swc+helpers@0.5.11_postcss@8.4.33/node_modules:/home/mrvin/newfolder/web/solid-workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parcel/lib/bin.js" "$@"
else
  exec node  "$basedir/../parcel/lib/bin.js" "$@"
fi
